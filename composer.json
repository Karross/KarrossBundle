{
  "name": "karross/framework",
  "description": "Admin framework for PHP to avoid reinventing the wheel.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Vincent Laurier",
      "email": "contact.karross@gmail.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/http-foundation": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/routing": "^7.3",
    "doctrine/persistence": "^4.0"
  },
  "require-dev": {
    "doctrine/doctrine-bundle": "^2.0",
    "doctrine/orm": "^3.0",
    "symfony/framework-bundle": "^7.3",
    "phpunit/phpunit": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "Karross\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TestedApp\\": "tests/Integration/TestedApp",
      "Tests\\": "tests/"
    }
  }
}
